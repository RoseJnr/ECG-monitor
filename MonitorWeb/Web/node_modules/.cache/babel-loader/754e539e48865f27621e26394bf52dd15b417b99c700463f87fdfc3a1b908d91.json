{"ast":null,"code":"import { Line } from 'vue-chartjs';\nimport { defineComponent, ref, watch, onMounted } from 'vue';\nimport { Chart as ChartJS, Title, Tooltip, Legend, LineElement, CategoryScale, LinearScale, PointElement } from 'chart.js';\nChartJS.register(Title, Tooltip, Legend, LineElement, CategoryScale, LinearScale, PointElement);\nexport default defineComponent({\n  name: 'RealtimeChart',\n  components: {\n    Line\n  },\n  props: {\n    chartData: {\n      type: Object,\n      required: true\n    }\n  },\n  setup(props) {\n    const lineChart = ref(null);\n    const options = {\n      responsive: true,\n      maintainAspectRatio: false\n    };\n    onMounted(() => {\n      watch(() => props.chartData, newData => {\n        console.log(\"New data: \", newData);\n        if (lineChart.value && lineChart.value.chartInstance) {\n          console.log('Updating chart instance data');\n          lineChart.value.chartInstance.data = JSON.parse(JSON.stringify(newData));\n          lineChart.value.chartInstance.update();\n        }\n      }, {\n        deep: true\n      });\n    });\n    return {\n      options,\n      lineChart\n    };\n  }\n});","map":{"version":3,"names":["Line","defineComponent","ref","watch","onMounted","Chart","ChartJS","Title","Tooltip","Legend","LineElement","CategoryScale","LinearScale","PointElement","register","name","components","props","chartData","type","Object","required","setup","lineChart","options","responsive","maintainAspectRatio","newData","console","log","value","chartInstance","data","JSON","parse","stringify","update","deep"],"sources":["C:\\Users\\roose\\Documents\\TCC\\ServerVue\\socketio-vue\\src\\components\\RealtimeChart.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <Line :data=\"chartData\" :options=\"options\" ref=\"lineChart\"></Line>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { Line } from 'vue-chartjs';\r\nimport { defineComponent, ref, watch, onMounted } from 'vue';\r\nimport { Chart as ChartJS,